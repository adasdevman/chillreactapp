{
  "expo": {
    "name": "ChillNow",
    "slug": "ChillnowApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chillnow.ios.client",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#222222"
      },
      "package": "com.chillnow.android.client"
    },
    "entryPoint": "./App.tsx",
    "extra": {
      "eas": {
        "projectId": "4524fd6b-0015-46e2-aba3-bff8bea68401"
      }
    },
    "owner": "adasfranck",
    "plugins": [
      "expo-font"
    ]
  }
}
