{
  "expo": {
    "name": "ChillnowApp",
    "slug": "ChillnowApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chillnow.ios.client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#222222"
      },
      "package": "com.chillnow.ios.client"
    },
    "entryPoint": "./App.tsx",
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c2d145fe-4e88-4005-b8e0-4f89602bda50"
      }
    }
  }
}
